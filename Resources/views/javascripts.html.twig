{% block admingenerator_form_javascripts %}

    {% javascripts package='formextensions_js' filter='?uglifyjs2' 
        output='assetic/js/compiled/admingeneratorformextensions/blueimp.jquery-file-upload.min.js'
        'bundles/admingeneratorformextensions/blueimp-javascript-templates/js/tmpl.js'
        'bundles/admingeneratorformextensions/blueimp-load-image/js/load-image.js'
        'bundles/admingeneratorformextensions/blueimp-load-image/js/load-image-ios.js'
        'bundles/admingeneratorformextensions/blueimp-load-image/js/load-image-orientation.js'
        'bundles/admingeneratorformextensions/blueimp-load-image/js/load-image-meta.js'
        'bundles/admingeneratorformextensions/blueimp-load-image/js/load-image-exif.js'
        'bundles/admingeneratorformextensions/blueimp-load-image/js/load-image-exif-map.js'
        'bundles/admingeneratorformextensions/blueimp-canvas-to-blob/js/canvas-to-blob.js'
        'bundles/admingeneratorformextensions/blueimp-file-upload/js/jquery.iframe-transport.js'
        'bundles/admingeneratorformextensions/blueimp-file-upload/js/jquery.fileupload.js'
        'bundles/admingeneratorformextensions/blueimp-file-upload/js/jquery.fileupload-process.js'
        'bundles/admingeneratorformextensions/blueimp-file-upload/js/jquery.fileupload-image.js'
        'bundles/admingeneratorformextensions/blueimp-file-upload/js/jquery.fileupload-validate.js'
        'bundles/admingeneratorformextensions/blueimp-file-upload/js/jquery.fileupload-ui.js'
    %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

    {% javascripts package='formextensions_js' filter='?uglifyjs2' 
        output='assetic/js/compiled/admingeneratorformextensions/form-extensions.min.js'
        'bundles/admingeneratorformextensions/bootstrap-collection/js/bootstrap-collection.js'
        'bundles/admingeneratorformextensions/bootstrap-datetimepicker/js/vendor/moment.js'
        'bundles/admingeneratorformextensions/bootstrap-datetimepicker/js/bootstrap-datetimepicker.js'
        'bundles/admingeneratorformextensions/bootstrap-select/js/bootstrap-select.js'
        'bundles/admingeneratorformextensions/collection-upload/js/collection-upload.js'
        'bundles/admingeneratorformextensions/double-list/js/double-list.js'
        'bundles/admingeneratorformextensions/elastic/js/jquery.elastic.js'
        'bundles/admingeneratorformextensions/knob/js/jquery.knob.js'
        'bundles/admingeneratorformextensions/minicolors/js/jquery.minicolors.js'
        'bundles/admingeneratorformextensions/select2/js/select2.js'
        'bundles/admingeneratorformextensions/single-upload/js/single-upload.js'
    %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

    <!-- locale -->
    {% if app.request.locale in ['ca', 'cs', 'da', 'de', 'es', 'et', 'eu', 'fr', 'gl', 'he', 'hr', 'hu', 'is', 'it', 'ja', 'lt', 'lv', 'mk', 'nl', 'no', 'pl', 'pt-BR', 'pt-PT', 'ro', 'ru', 'sk', 'sv', 'tr', 'ua', 'vi', 'zh-CN', 'zh-TW'] %}
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/select2/js/i18n/select2_locale_' ~ app.request.locale ~ '.js', 'formextensions_js') }}"></script>
    {% endif %}

    {% if app.request.locale in ['cs', 'de', 'es', 'eu', 'it', 'nl', 'pl', 'pt-BR', 'ru', 'zh-CN'] %}
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/bootstrap-select/js/i18n/defaults-' ~ app.request.locale ~ '.js', 'formextensions_js') }}"></script>
    {% endif %}

    {% if app.request.locale in ['ar-ma', 'ar', 'bg', 'br', 'bs', 'ca', 'cs', 'cv', 'da', 'de', 'el', 'en-au', 'en-ca', 'en-gb', 'eo', 'es', 'et', 'eu', 'fa', 'fi', 'fr-ca', 'fr', 'gl', 'he', 'hi', 'hr', 'hu', 'id', 'is', 'it', 'ja', 'ka', 'ko', 'lt', 'lv', 'ml', 'mr', 'ms-mv', 'nb', 'ne', 'nl', 'nn', 'pl', 'pt-BR', 'pt', 'ro', 'ru', 'sk', 'sl', 'sq', 'sv', 'th', 'tr', 'tzm-la', 'tzm', 'uk', 'uz', 'vn', 'zh-CN', 'zh-TW'] %}
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/bootstrap-datetimepicker/js/locales/bootstrap-datetimepicker.' ~ app.request.locale ~ '.js', 'formextensions_js') }}"></script>
    {% endif %}

{% endblock admingenerator_form_javascripts %}
