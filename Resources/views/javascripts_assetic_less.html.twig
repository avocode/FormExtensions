{% block admingenerator_form_javascripts %}
    <!-- blueimp.jquery-file-upload.min.js  -->
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/blueimp-javascript-templates/js/tmpl.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/blueimp-load-image/js/load-image.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/blueimp-canvas-to-blob/js/canvas-to-blob.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/blueimp-file-upload/js/jquery.iframe-transport.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/blueimp-file-upload/js/jquery.fileupload.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/blueimp-file-upload/js/jquery.fileupload-process.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/blueimp-file-upload/js/jquery.fileupload-image.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/blueimp-file-upload/js/jquery.fileupload-validate.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/blueimp-file-upload/js/jquery.fileupload-ui.js') }}"></script>

    <!-- form-extensions.min.js -->
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/bootstrap-collection/js/bootstrap-collection.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/bootstrap-datetimepicker/js/vendor/moment.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/bootstrap-datetimepicker/js/bootstrap-datetimepicker.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/bootstrap-select/js/bootstrap-select.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/collection-upload/js/collection-upload.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/double-list/js/double-list.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/elastic/js/jquery.elastic.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/knob/js/jquery.knob.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/minicolors/js/jquery.minicolors.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/select2/js/select2.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/single-upload/js/single-upload.min.js') }}"></script>

    <!-- locale -->
    {% if app.request.locale in ['ca', 'cs', 'da', 'de', 'es', 'et', 'eu', 'fr', 'gl', 'he', 'hr', 'hu', 'is', 'it', 'ja', 'lt', 'lv', 'mk', 'nl', 'no', 'pl', 'pt-BR', 'pt-PT', 'ro', 'ru', 'sk', 'sv', 'tr', 'ua', 'vi', 'zh-CN', 'zh-TW'] %}
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/select2/js/i18n/select2_locale_' ~ app.request.locale ~ '.js') }}"></script>
    {% endif %}

    {% if app.request.locale in ['cs', 'de', 'es', 'eu', 'it', 'nl', 'pl', 'pt-BR', 'ru', 'zh-CN'] %}
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/bootstrap-select/js/i18n/defaults-' ~ app.request.locale ~ '.js') }}"></script>
    {% endif %}

    {% if app.request.locale in ['ar-ma', 'ar', 'bg', 'br', 'bs', 'ca', 'cs', 'cv', 'da', 'de', 'el', 'en-au', 'en-ca', 'en-gb', 'eo', 'es', 'et', 'eu', 'fa', 'fi', 'fr-ca', 'fr', 'gl', 'he', 'hi', 'hr', 'hu', 'id', 'is', 'it', 'ja', 'ka', 'ko', 'lt', 'lv', 'ml', 'mr', 'ms-mv', 'nb', 'ne', 'nl', 'nn', 'pl', 'pt-BR', 'pt', 'ro', 'ru', 'sk', 'sl', 'sq', 'sv', 'th', 'tr', 'tzm-la', 'tzm', 'uk', 'uz', 'vn', 'zh-CN', 'zh-TW'] %}
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorformextensions/bootstrap-datetimepicker/js/locales/bootstrap-datetimepicker.' ~ app.request.locale ~ '.js') }}"></script>
    {% endif %}
{% endblock admingenerator_form_javascripts %}
